{
  "name": "Polydex",
  "productName": "Polydex",
  "version": "0.0.1",
  "description": "File Indexer",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --asar=false --platform=darwin --arch=x64 --prune=true --out=release-builds",
    "package-win": "electron-packager . --overwrite --asar=false --platform=win32 --arch=ia32 --icon=assets/polydex256.ico --prune=true --out=release-builds --version-string.CompanyName=DiegoVillagrasa --version-string.FileDescription=Executable --version-string.ProductName=\"Polydex\"",
    "package-linux": "electron-packager . --overwrite --asar=false --platform=linux --arch=x64 --icon=assetspolydex256.png --prune=true --out=release-builds"
  },
  "repository": "https://github.com/diegovillagrasa/polydex",
  "keywords": [
    "Polydex"
  ],
  "author": "Diego Villagrasa",
  "license": "Apache",
  "devDependencies": {
    "electron": "^1.4.1"
  },
  "dependencies": {
    "electron-json-storage": "^3.0.1",
    "electron-require": "^0.3.0",
    "google-auth-library": "^0.10.0",
    "googleapis": "^26.0.1",
    "nedb": "^1.8.0",
    "request": "^2.80.0",
    "stat-mode": "^0.2.2"
  }
}
